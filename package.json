{
  "name": "@dailybruin/lux",
  "version": "0.0.0-semantically-released",
  "author": "Daily Bruin Online <online@dailybruin.com>",
  "description": "The Daily Bruin's component library.",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dailybruin/lux.git"
  },
  "bugs": {
    "url": "https://github.com/dailybruin/lux/issues"
  },
  "homepage": "https://github.com/dailybruin/lux",
  "keywords": [
    "design system",
    "journalism"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "tslint"
    ],
    "*": [
      "eclint check"
    ]
  },
  "scripts": {
    "start": "docz dev",
    "build": "rollup -c",
    "build:docs": "docz build",
    "prepublish": "yarn build",
    "commit": "git-cz",
    "dev:chapters": "parcel src/layouts/chapter-based.html --open",
    "dev:grid": "parcel src/layouts/chapter-based.html --open",
    "test": "jest",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.4",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@types/lodash": "^4.14.116",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/webfontloader": "^1.6.29",
    "cpx": "^1.5.0",
    "docz": "0.12.5",
    "docz-plugin-netlify": "^1.1.4",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "gatsby-plugin-emotion": "^2.0.5",
    "lodash": "^4.17.11",
    "parcel-bundler": "1.10.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "rollup-plugin-img": "^1.1.0",
    "slugify": "^1.3.1",
    "typescript": "3.1.2",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/jest": "23.3.5",
    "commitizen": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "eclint": "2.8.0",
    "gatsby": "2.0.21",
    "husky": "1.1.1",
    "jest": "23.6.0",
    "jest-emotion": "9.2.11",
    "lint-staged": "7.3.0",
    "react-testing-library": "5.2.0",
    "rollup": "0.66.6",
    "rollup-plugin-typescript2": "0.17.1",
    "semantic-release": "15.9.17",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0"
  }
}
