{
  "name": "@dailybruin/lux",
  "version": "0.0.0-development",
  "author": "Daily Bruin Online <online@dailybruin.com>",
  "description": "The Daily Bruin's component library.",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/dailybruin/lux.git"
  },
  "bugs": {
    "url": "https://github.com/dailybruin/lux/issues"
  },
  "homepage": "https://github.com/dailybruin/lux",
  "keywords": [
    "design system",
    "journalism"
  ],
  "main": "src/index.ts",
  "files": [
    "src"
  ],
  "directories": {
    "test": "tests"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx,json,css,md}": [
        "prettier --write",
        "git add"
      ],
      "*.{ts,tsx}": [
        "tslint"
      ],
      "*": [
        "eclint check"
      ]
    },
    "ignore": [
      "**/dist/*",
      "*.test.*"
    ]
  },
  "scripts": {
    "start": "docz dev",
    "build": "rollup -c",
    "build:docs": "docz build",
    "commit": "git-cz",
    "test": "jest",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "dependencies": {
    "@types/react": "16.4.18",
    "@types/react-dom": "16.0.9",
    "@types/react-helmet": "^5.0.7",
    "@types/webfontloader": "^1.6.29",
    "emotion": "^9.2.12",
    "emotion-server": "^9.2.12",
    "gatsby-plugin-emotion": "^2.0.5",
    "plyr": "^3.4.4",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-emotion": "^9.2.12",
    "react-helmet": "^5.2.0",
    "react-plyr": "^2.1.1",
    "slugify": "^1.3.1",
    "typescript": "3.1.5",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "commitizen": "3.0.4",
    "cz-conventional-changelog": "2.1.0",
    "docz": "0.12.5",
    "docz-plugin-css": "0.11.0",
    "docz-plugin-netlify": "1.1.4",
    "eclint": "2.8.1",
    "gatsby": "2.0.35",
    "husky": "1.1.2",
    "jest": "23.6.0",
    "jest-emotion": "9.2.11",
    "lint-staged": "8.0.4",
    "react-testing-library": "5.2.3",
    "semantic-release": "15.10.7",
    "travis-deploy-once": "5.0.9",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "3.6.0"
  }
}
